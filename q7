// 7. Program to evaluate a prefix expression
#include <stdio.h>
#include <ctype.h>
#include <string.h>
#define MAX 100
int stack[MAX], top = -1;
void push(int x) { stack[++top] = x; }
int pop() { return stack[top--]; }
int main() {
    char exp[MAX];
    int i, op1, op2;
    printf("Enter prefix expression: ");
    gets(exp);
    int len = strlen(exp);
    for (i = len - 1; i >= 0; i--) {
        if (isdigit(exp[i]))
            push(exp[i] - '0');
        else {
            op1 = pop();
            op2 = pop();
            switch (exp[i]) {
                case '+': push(op1 + op2); break;
                case '-': push(op1 - op2); break;
                case '*': push(op1 * op2); break;
                case '/': push(op1 / op2); break;
            }
        }
    }
    printf("Result: %d\n", pop());
    return 0;
}

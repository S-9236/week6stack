// 6. Program to evaluate a postfix expression
#include <stdio.h>
#include <ctype.h>
#define MAX 100
int stack[MAX], top = -1;
void push(int x) { stack[++top] = x; }
int pop() { return stack[top--]; }
int main() {
    char exp[MAX];
    int i, op1, op2;
    printf("Enter postfix expression: ");
    gets(exp);
    for (i = 0; exp[i]; i++) {
        if (isdigit(exp[i]))
            push(exp[i] - '0');
        else {
            op2 = pop();
            op1 = pop();
            switch (exp[i]) {
                case '+': push(op1 + op2); break;
                case '-': push(op1 - op2); break;
                case '*': push(op1 * op2); break;
                case '/': push(op1 / op2); break;
            }
        }
    }
    printf("Result: %d\n", pop());
    return 0;
}
